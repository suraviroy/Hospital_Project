{
  "expo": {
    "name": "IpcrConnect",
    "slug": "IpcrConnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pictures/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pictures/sp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pictures/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "MANAGE_EXTERNAL_STORAGE"
      ],
      "package": "com.tiasha21.Patient"
    },
    "web": {
      "favicon": "./assets/images/lab1.jpeg"
    }
  }
}
